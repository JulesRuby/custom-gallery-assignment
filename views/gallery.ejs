<%- include('partials/head') %>
<%- include('partials/header') %>

<body>
  <main>
    <h2><%= content %></h2>
    
    <% gallery.forEach(object) { %>
      <figure>
        <a href="images/lrg/<%= object.imageID %>">
          <img src="<%= images/sm/<%= object.fileName %>" alt="<%= object.title %>">
        </a>
        <figcaption>
          <h3><%= object.attribution.credit %></h3>
        </figcaption>
      </figure>
      <% } %>
    
    
  </main>
  <%- include('partials/footer') %>
</body>
</html>


